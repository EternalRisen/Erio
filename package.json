{
  "name": "erio",
  "version": "1.0.0",
  "description": "A Discord Bot for the Yeet Server",
  "main": "start",
  "scripts": {
    "herokustart": "node -r dotenv/config www-dist/server/app.js",
    "start": "node -r dotenv/config bot --production",
    "devstart": "node -r dotenv/config bot --development",
    "watch": "webpack --watch --mode development",
    "compile": "tslint *.ts && node -r dotenv/config build",
    "prodpack": "webpack --mode production",
    "devpack": "webpack --mode development",
    "build": "npm run compile && npm run prodpack",
    "devbuild": "npm run compile && npm run devpack",
    "dev": "npm run compile && concurrently \"npm:watch\" \"npm:devstart\" \"sleep 3  && open-cli http://localhost:3000\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaboijd/erio.git"
  },
  "author": "JD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yaboijd/erio/issues"
  },
  "homepage": "https://github.com/yaboijd/erio#readme",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.2",
    "@types/pg": "^7.14.5",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/ws": "^7.2.7",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^3.6.0",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-loader": "^4.3.0",
    "got": "^11.8.0",
    "html-webpack-plugin": "^3.2.0",
    "list-react-files": "^0.2.0",
    "open-cli": "^6.0.1",
    "opusscript": "0.0.7",
    "pg": "^8.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "style-loader": "^1.3.0",
    "sucrase": "^3.16.0",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "ytdl-core": "^3.4.2"
  }
}
