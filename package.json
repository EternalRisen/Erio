{
  "name": "erio",
  "version": "1.0.0",
  "description": "A Discord Bot for the Yeet Server",
  "main": "start",
  "scripts": {
    "start": "node -r dotenv/config bot",
    "serve": "node -r dotenv/config www-dist/server/app.js",
    "watch": "webpack --watch --mode development",
    "compile": "tslint *.ts && node build",
    "build": "webpack --mode development && node build",
    "dev": "concurrently \"npm:watch\" \"npm:serve\" \"sleep 3 && npm run compile && open-cli http://localhost:3000\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaboijd/erio.git"
  },
  "author": "JD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yaboijd/erio/issues"
  },
  "homepage": "https://github.com/yaboijd/erio#readme",
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.18",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/ws": "^7.2.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.6.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ffmpeg-static": "",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "list-react-files": "^0.2.0",
    "open-cli": "^6.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
